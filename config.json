{
    "embedding_model": "all-MiniLM-L6-v2",
    "prompt_type": "cot",
    "llm_model": "google/flan-t5-large",
    "top_k": 3,
    "max_context_length": 1000,
    "temperature": 0.7,
    "max_tokens": 150,
    "log_level": "INFO",
    "data_paths": {
        "passages": "data/passages.parquet",
        "test_questions": "data/test.parquet"
    },
    "output_paths": {
        "chroma_db": "./chroma_db",
        "results": "outputs/evaluation_results.json",
        "logs": "logs/rag_system.log"
    },
    "evaluation": {
        "batch_size": 10,
        "save_detailed_results": true
    }
}